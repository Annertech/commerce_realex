{
    "name": "drupal/commerce_realex",
    "description": "Commerce Realex / Global Payments.",
    "type": "drupal-module",
    "homepage": "https://drupal.org/project/commerce_realex",
    "authors": [
        {
            "name": "Stella Power (stella)",
            "homepage": "https://www.drupal.org/u/stella",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://drupal.org/project/issues/commerce_realex",
        "irc": "irc://irc.freenode.org/drupal-contribute",
        "source": "https://cgit.drupalcode.org/commerce_realex"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "repositories": {
